{
    "manifest_version": 3,
    "name": "PassSecure",
    "version": "1.0",
    "description": "A basic password manager extension.",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "http://127.0.0.1:5000/*"
    ]
}
